{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["react-app", "react-app/jest", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "import/order": ["error", { "newlines-between": "always" }]
  }
}
